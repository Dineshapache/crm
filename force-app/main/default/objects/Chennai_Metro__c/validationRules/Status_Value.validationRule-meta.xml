<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_Value</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Account_Status__c,&apos;Closed&apos;),ISBLANK(End_Date__c))</errorConditionFormula>
    <errorDisplayField>Account_Status__c</errorDisplayField>
    <errorMessage>End Date Should not be null</errorMessage>
</ValidationRule>
